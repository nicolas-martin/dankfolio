{
  "title": "Database Performance Analysis",
  "uid": "database-performance",
  "editable": true,
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Database Query Count by Table",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{span.db.collection.name=\"coins\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": ["count"]
        }
      }
    },
    {
      "id": 2,
      "title": "Average Query Duration by Operation",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{span.db.query.text!=\"\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "spanNulls": false,
            "pointSize": 5,
            "showPoints": "auto"
          },
          "unit": "ms"
        }
      }
    },
    {
      "id": 3,
      "title": "Query Operations Breakdown",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "tempo",
            "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{span.db.operation.name!=\"\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "options": {
        "pieType": "donut",
        "displayLabels": ["name", "percent"],
        "legendDisplayMode": "list",
        "legendPlacement": "right"
      }
    },
    {
      "id": 4,
      "title": "Recent Database Queries",
      "type": "logs",
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 8
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{span.db.query.text!=\"\"}",
          "queryType": "traceql",
          "limit": 20,
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    }
  ]
}