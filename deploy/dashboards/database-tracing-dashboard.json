{
  "title": "Database Repository Performance",
  "uid": "database-repository-perf",
  "editable": true,
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Repository Methods Performance",
      "type": "table",
      "gridPos": {
        "h": 14,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "limit": 50,
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "description": "Repository method calls with their execution times. Click on any trace to see details including SQL queries.",
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Duration",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Repository Method Calls by Table",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A",
          "limit": 200
        }
      ],
      "options": {
        "pieType": "donut",
        "displayLabels": ["name", "value"],
        "legendDisplayMode": "table",
        "legendPlacement": "right",
        "legendValues": ["value", "percent"],
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Count GetByField"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "GetByField"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Count Get"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Get"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Count List"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "List"
              }
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "attributes",
            "fields": {
              "db.table": "Table",
              "name": "Method"
            }
          }
        },
        {
          "id": "concatenateFields",
          "options": {
            "sourceFields": ["Name", "Table"],
            "targetField": "Method_Table",
            "separator": " - "
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Method_Table": {
                "operation": "groupby"
              },
              "Duration": {
                "operation": "count"
              }
            }
          }
        }
      ]
    },
    {
      "id": 3,
      "title": "Average Repository Method Duration by Table",
      "type": "bargauge",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A",
          "limit": 200
        }
      ],
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": ["mean"]
        },
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2,
          "min": 0,
          "max": 20,
          "noValue": "No data"
        },
        "overrides": []
      },
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "attributes",
            "fields": {
              "db.table": "Table"
            }
          }
        },
        {
          "id": "concatenateFields",
          "options": {
            "sourceFields": ["Name", "Table"],
            "targetField": "Method_Table",
            "separator": " - "
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Method_Table": {
                "operation": "groupby"
              },
              "Duration": {
                "operation": "mean",
                "aggregations": ["mean"]
              }
            }
          }
        }
      ]
    },
    {
      "id": 4,
      "title": "Slow Repository Operations (>10ms)",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "limit": 20,
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\" && duration > 10ms}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Duration",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    }
  ]
}