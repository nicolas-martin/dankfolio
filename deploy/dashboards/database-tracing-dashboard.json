{
  "title": "Database Repository Performance",
  "uid": "database-repository-perf",
  "editable": true,
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Repository Methods Performance",
      "type": "table",
      "gridPos": {
        "h": 14,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "limit": 50,
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "description": "Repository method calls with their execution times. Click on any trace to see details including SQL queries.",
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Duration",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Repository Method Calls",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A",
          "limit": 200,
          "tableType": "spans"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["count"]
        },
        "orientation": "auto",
        "textMode": "auto",
        "graphMode": "none",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow", 
                "value": 100
              },
              {
                "color": "red",
                "value": 500
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Average Repository Method Duration",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A",
          "limit": 200,
          "tableType": "spans"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["mean"]
        },
        "orientation": "auto",
        "textMode": "auto",
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Slow Repository Operations (>10ms)",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "limit": 20,
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\" && duration > 10ms}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Duration",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    }
  ]
}