{
  "title": "Database Trace",
  "uid": "database-trace",
  "editable": true,
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Repository Method Traces",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "limit": 100,
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "description": "All repository method calls with their details",
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Start time",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Repository Operations Summary",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\"}",
          "queryType": "traceql",
          "refId": "A",
          "limit": 200
        }
      ],
      "description": "Overall repository method performance metrics",
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "count",
            "mean"
          ]
        },
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "graphMode": "none",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration (count)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "short"
              },
              {
                "id": "displayName",
                "value": "Total Calls"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "blue",
                      "value": null
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Duration (mean)"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Avg Duration"
              }
            ]
          }
        ]
      }
    },
    {
      "id": 3,
      "title": "Slow Repository Operations (>10ms)",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "datasource": {
        "type": "tempo",
        "uid": "P214B5B846CF3925F"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "P214B5B846CF3925F"
          },
          "limit": 20,
          "query": "{resource.service.name=\"dankfolio-backend\" && name=~\"repository.*\" && duration > 10ms}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Duration",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    }
  ]
}
