{
  "title": "Database Query Analysis",
  "editable": true,
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
          "text": "Tempo",
          "value": "P214B5B846CF3925F"
        },
        "hide": 0,
        "includeAll": false,
        "label": "Tempo Datasource",
        "multi": false,
        "name": "tempo",
        "options": [],
        "query": "tempo",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      },
      {
        "current": {
          "selected": false,
          "text": "Mimir",
          "value": "mimir"
        },
        "hide": 0,
        "includeAll": false,
        "label": "Metrics Datasource",
        "multi": false,
        "name": "metrics",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "panels": [
    {
      "title": "Recent Database Queries",
      "type": "table",
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 100,
          "query": "{span.db.orm=\"gorm\"} | select(span.db.query.text, span.db.collection.name, span.db.operation.name, duration)",
          "queryType": "traceql",
          "refId": "A",
          "tableType": "spans"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Start time"
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "span.db.query.text"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 600
              },
              {
                "id": "displayName",
                "value": "SQL Query"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "span.db.collection.name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Table"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "span.db.operation.name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Operation"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "displayName",
                "value": "Duration"
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Query Performance by Table",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 1000,
          "query": "{span.db.orm=\"gorm\"} | avg(duration) by (span.db.collection.name)",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "displayName": "${__field.labels.span.db.collection.name}",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 100
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "title": "Slowest Queries",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 20,
          "query": "{span.db.orm=\"gorm\" && duration > 50ms} | select(span.db.query.text, duration, span.db.collection.name)",
          "queryType": "traceql",
          "refId": "A",
          "tableType": "spans"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "duration"
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "span.db.query.text"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 500
              },
              {
                "id": "displayName",
                "value": "SQL Query"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "displayName",
                "value": "Duration"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "span.db.collection.name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Table"
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Query Types Distribution",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 1000,
          "query": "{span.db.orm=\"gorm\"} | count() by (span.db.operation.name)",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "options": {
        "pieType": "donut",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Database Load Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 1000,
          "query": "{span.db.orm=\"gorm\"} | rate()",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
          },
          "unit": "ops"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      }
    }
  ]
}