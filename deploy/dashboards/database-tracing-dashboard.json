{
  "title": "Database Query Analysis",
  "editable": true,
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
          "text": "Tempo",
          "value": "P214B5B846CF3925F"
        },
        "hide": 0,
        "includeAll": false,
        "label": "Tempo Datasource",
        "multi": false,
        "name": "tempo",
        "options": [],
        "query": "tempo",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "panels": [
    {
      "title": "Database Queries",
      "type": "traces",
      "gridPos": {
        "h": 14,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 50,
          "query": "{span.db.orm=\"gorm\"}",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "description": "Click on any trace to see the full SQL query in span attributes:\n- db.query.text: Full SQL\n- db.collection.name: Table name\n- db.operation.name: Operation type"
    },
    {
      "title": "Slow Database Queries (>50ms)",
      "type": "traces",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "tempo",
        "uid": "${tempo}"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${tempo}"
          },
          "limit": 20,
          "query": "{span.db.orm=\"gorm\" && duration > 50ms}",
          "queryType": "traceql",
          "refId": "A"
        }
      ]
    }
  ]
}