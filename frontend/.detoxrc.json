{
  "skipLegacyWorkersInjection": true,
  "apps": {
    "ios.debug": {
      "type": "ios.app",
      "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/dankfolio.app",
      "build": "xcodebuild -workspace ios/dankfolio.xcworkspace -scheme dankfolio -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 16 Pro Max"
      }
    }
  },
  "configurations": {
    "ios.sim.debug": {
      "device": "simulator",
      "app": "ios.debug"
    }
  },
  "testRunner": {
    "args": {
      "config": "e2e/jest.config.js",
      "$0": "jest"
    },
    "jest": {
      "setupTimeout": 120000
    }
  }
}
