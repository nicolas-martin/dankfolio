{
  "testRunner": "jest",
  "runnerConfig": "e2e/jest.config.js",
  "skipLegacyWorkersInjection": true,
  "apps": {
    "ios.debug": {
      "type": "ios.app",
      "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/dankfolio.app",
      "build": "npx expo run:ios --configuration Debug --device \"iPhone 16 Pro Max\" --no-bundler"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 16 Pro Max"
      }
    }
  },
  "configurations": {
    "ios.sim.debug": {
      "device": "simulator",
      "app": "ios.debug"
    }
  }
}
